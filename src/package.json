{
  "name": "AAA-21",
  "version": "0.0.1",
  "description": "Offical website for the 2021-winter graduation from Aarhus School of Architecture",
  "scripts": {
    "bundle-main": "esbuild index.main.js --bundle --minify --outfile=../dist/bundle.js",
    "bundle-mobile": "esbuild index.mobile.js --bundle --minify --outfile=../dist/bundle.mobile.js",
    
    "terser-main": "terser ../dist/bundle.js --comments -c -m -o ../dist/bundle.js",
    
    "bundle-main-dev": "esbuild index.main.js --bundle --outfile=../dist/bundle.js",
    "bundle-mobile-dev": "esbuild index.mobile.js --bundle --outfile=../dist/bundle.mobile.js",
    
    "bundle": "npm run bundle-main && npm run bundle-mobile && npm run terser-main",
    "bundle-dev": "npm run bundle-main-dev && npm run bundle-mobile-dev",
    
    "watch": "watchexec --exts js,css,html -i /*.bundle.js/ npm run bundle-dev"
  },
  "keywords": [],
  "author": "JÃ¸rgen Skogmo <js@dearstudio.dk>",
  "license": "Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)",
  "dependencies": {
    "es6-tween": "^5.5.11",
    "esbuild": "^0.8.15",
    "fabric": "^4.2.0",
    "terser": "^5.5.1",
    "three": "^0.123.0"
  }
}
