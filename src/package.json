{
  "name": "AAA-21",
  "version": "0.0.1",
  "description": "Offical website for the 2021-winter graduation from Aarhus School of Architecture",
  "scripts": {
    "bundle-main": "esbuild index.main.js --bundle --minify --outfile=../dist/bundle.js --banner='/* comment */'",
    "bundle-mobile": "esbuild index.mobile.js --bundle --minify --outfile=../dist/bundle.mobile.js --banner='/* comment */'",
    "bundle": "npm run bundle-main && npm run bundle-mobile",
    "watch": "watchexec --exts js,css,html -i /*.bundle.js/ npm run bundle"
  },
  "keywords": [],
